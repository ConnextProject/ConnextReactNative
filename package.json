{
  "name": "ConnextReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "node_modules/.bin/react-native run-android",
    "ios": "node_modules/.bin/react-native run-ios",
    "start": "node_modules/.bin/react-native start",
    "clean-start": "yarn run clean:metro && node_modules/.bin/react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "bash ops/post-install.sh",
    "clean:node_modules": "rm -rf node_modules",
    "clean:metro": "rm -rf /tmp/metro-* && watchman watch-del-all",
    "clean:ios": "cd ios && rm -rf build && pod cache clean --all && rm -rf Pods && cd .."
  },
  "dependencies": {
    "@connext/client": "4.0.5",
    "@react-native-community/async-storage": "^1.7.1",
    "@tradle/react-native-http": "^2.0.1",
    "assert": "^1.5.0",
    "asyncstorage-down": "^4.2.0",
    "base-64": "^0.1.0",
    "browserify-zlib": "^0.1.4",
    "buffer": "^4.9.2",
    "console-browserify": "^1.2.0",
    "constants-browserify": "^1.0.0",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.2.0",
    "ethers": "^4.0.40",
    "events": "^1.1.1",
    "https-browserify": "0.0.1",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-browser-polyfill": "^0.1.2",
    "react-native-crypto": "^2.2.0",
    "react-native-level-fs": "^3.0.1",
    "react-native-os": "^1.2.6",
    "react-native-randombytes": "^3.5.3",
    "react-native-tcp": "^3.3.2",
    "react-native-udp": "^2.6.1",
    "readable-stream": "^1.0.33",
    "rn-nodeify": "^10.2.0",
    "secp256k1": "3.8.0",
    "stream-browserify": "^1.0.0",
    "string_decoder": "^0.10.31",
    "timers-browserify": "^1.4.2",
    "tty-browserify": "0.0.0",
    "url": "^0.10.3",
    "util": "^0.10.4",
    "vm-browserify": "0.0.4",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.7.7",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/eccrypto": "^1.1.1",
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.2",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@types/secp256k1": "^3.5.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "react-native": {
    "zlib": "browserify-zlib",
